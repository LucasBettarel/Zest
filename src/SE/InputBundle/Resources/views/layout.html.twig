{% extends "::layout.html.twig" %}

{% block title %}
  Input - {{ parent() }}
{% endblock %}

{% block menu %}
	<div class="menu-heading">
		<h4>Input Config Panel</h4>
	</div>
	<div class="menu-body">
		{{ render(controller("SEInputBundle:Entry:menu")) }}
	</div>
{% endblock %}

{% block body %}

